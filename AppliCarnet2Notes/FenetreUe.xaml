<Window x:Class="CarnetDeNotes.FenetreUe"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CarnetDeNotes"
        mc:Ignorable="d"
        Title="FenetreUe" Height="300" Width="300">
    <Grid>
        <TextBox x:Name="txtNom" Text="{Binding Nom}" HorizontalAlignment="Left" Height="23" Margin="76,97,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" RenderTransformOrigin="0.011,0.538"/>
        <Label Content="  Nom de l'UE" HorizontalAlignment="Left" Margin="76,66,0,0" VerticalAlignment="Top" Width="102"/>
        <Button x:Name="Ajouter" Content="Ajouter" HorizontalAlignment="Left" Margin="76,145,0,0" VerticalAlignment="Top" Width="102" Height="24" Click="Click_Ajouter"/>
        <Label Content="Ajouter une UE" HorizontalAlignment="Left" Margin="43,10,0,0" VerticalAlignment="Top" Width="204" FontFamily="Stencil" FontSize="24"/>

    </Grid>
</Window>
